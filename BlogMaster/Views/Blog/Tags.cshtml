
@{
    Layout = "~/Views/Shared/_AdministratorLayout.cshtml";
}
@model IEnumerable<TagResponseDto>;

<div class="container overflow-x-scroll">
    <table class="table" style="z-index: 1;">
        <thead class="table-light sticky-top">
            <tr>
                <th scope="col">TagEn</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @foreach (TagResponseDto tag in Model)
            {
                <tr>
                    <td>@(tag.TagNameEn)</td>
                    <td>
                        <a href="/create-tag?TagId=@(tag.TagId)&TagNameEn=@(Uri.EscapeDataString(tag.TagNameEn ?? ""))" class="btn btn-primary">Edit</a>
                    </td>


                </tr>
            }
        </tbody>
    </table>
</div>

